{
  "name": "should-enzyme",
  "version": "0.0.1",
  "description": "Useful functions for testing React Components with enzyme",
  "scripts": {
    "test": "mocha -r should --compilers js:babel-core/register src/*spec.js src/**/*spec.js",
    "test:w": "npm run js-style && mocha -r should -w --compilers js:babel-core/register src/*spec.js src/**/*spec.js",
    "js-style": "eslint ./src/",
    "js-style-fix": "eslint ./src/ --fix",
    "compile": "babel -d lib/ src/ --ignore *spec.js",
    "prepublish": "npm run test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rkotze/should-enzyme"
  },
  "keywords": [
    "react",
    "enzyme",
    "should",
    "assert",
    "testing"
  ],
  "main": "lib/index.js",
  "license": {
    "type": "MIT",
    "url": "https://github.com/rkotze/should-enzyme/blob/master/LICENSE"
  },
  "author": "Richard Kotze <richkotze@outlook.com>",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.7.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.11.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-google": "^0.6.0",
    "mocha": "^3.0.2",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "should": "^11.1.0"
  },
  "bugs": {
    "url": "https://github.com/rkotze/should-enzyme/issues"
  },
  "files": [
    "lib/*",
    "readme.md"
  ]
}
